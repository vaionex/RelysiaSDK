{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "new-cap":"off",
      "no-invalid-this":"off",
      "require-jsdoc": "off",
      "max-len":"off"
    }
}
